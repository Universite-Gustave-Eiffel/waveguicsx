# shortuts to switch between real/complex modes
alias complex='source /usr/local/bin/dolfinx-complex-mode'
alias real='source /usr/local/bin/dolfinx-real-mode'

alias python=python3
alias py=python
alias ipy='ipython'
alias c=clear
alias ll='pwd; ls --time-style="+%Y-%m-%dT%H:%M:%S" -l1hv --color'




